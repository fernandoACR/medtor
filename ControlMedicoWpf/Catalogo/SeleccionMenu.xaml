<Window x:Class="ControlMedicoWpf.Catalogo.SeleccionMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlMedicoWpf.Catalogo"
        mc:Ignorable="d"
        Title="SeleccionMenu" Height="225" Width="800" Background="#FF64AF9E" WindowStartupLocation="CenterOwner" WindowStyle="None">
    <Grid>
        <Button Height="100" Width="100" HorizontalAlignment="Center" HorizontalContentAlignment="Center">
            <Button.Template>
                <ControlTemplate>
                    <Image Source="ControlMedicoWpf/Resources/Images/doctor.png"/>
                </ControlTemplate>
            </Button.Template>
        </Button>

        <Button Height="100" Width="100" HorizontalAlignment="Left">
            <Button.Template>
                <ControlTemplate>
                    <Image Source="ControlMedicoWpf/Resources/Images/doctor.png"/>
                </ControlTemplate>
            </Button.Template>
        </Button>
        
    </Grid>
</Window>
